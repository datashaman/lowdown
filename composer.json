{
    "name": "datashaman/lowdown",
    "authors": [
        {
            "name": "Marlin Forbes",
            "email": "marlinf@datashaman.com"
        }
    ],
    "description": "Documentation generator for PHP projects",
    "keywords": ["documentation-generator", "documentation", "reflection", "vuejs"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/datashaman/lowdown/issues",
        "source": "https://github.com/datashaman/lowdown"
    },
    "require": {
        "php": "^7.2",
        "cache/redis-adapter": "^1.0",
        "erusev/parsedown": "^1.7",
        "hanneskod/classtools": "~1.0",
        "illuminate/log": "5.8.*",
        "knplabs/github-api": "^2.11",
        "laravel-zero/framework": "5.8.*",
        "php-http/guzzle6-adapter": "^1.1",
        "phpdocumentor/reflection-docblock": "4.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\Lowdown\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datashaman\\Lowdown\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["lowdown"]
}
